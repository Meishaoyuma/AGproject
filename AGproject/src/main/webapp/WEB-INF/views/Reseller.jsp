        <script>
      window.markers = new Array();
      var activeInfoWindow;
      (function($) {
        /*
        *  render_map
        *  This function will render a Google Map onto the selected jQuery element
        */
        function render_map( $el ) {
          var $markers = $('#resellers-loop').find('.marker');
          var args = {
            zoom    : 14,
            center    : new google.maps.LatLng(0, 0),
            mapTypeId : google.maps.MapTypeId.ROADMAP
          };
          // create map
          var map = new google.maps.Map( $el[0], args);

          // add a markers reference
          map.markers = [];
          window.myMarkers = new Array();
          // add markers
          window.counter = 1;
          $markers.each(function(){
              add_marker( $(this), map );
          });
          if (FWP.facets['reseller_radius_miles'].length > 0) {
            var userlat = parseFloat(FWP.facets['reseller_radius_miles'][0]);
            var userlng = parseFloat(FWP.facets['reseller_radius_miles'][1]);
            var enteredlatlng = new google.maps.LatLng( userlat, userlng );
            var pincolor = '#005F9E';
            var pinicon = pinSymbolPerson(pincolor);

            // create marker
            var usermarker = new google.maps.Marker({
              position  : enteredlatlng,
              animation : google.maps.Animation.DROP,
              map       : map,
              icon: pinicon,
            });
          }
          if (FWP.facets['reseller_radius_km'].length > 0) {
            var userlat = parseFloat(FWP.facets['reseller_radius_km'][0]);
            var userlng = parseFloat(FWP.facets['reseller_radius_km'][1]);
            var enteredlatlng = new google.maps.LatLng( userlat, userlng );
            var pincolor = '#005F9E';
            var pinicon = pinSymbolPerson(pincolor);

            // create marker
            var usermarker = new google.maps.Marker({
              position  : enteredlatlng,
              animation : google.maps.Animation.DROP,
              map       : map,
              icon: pinicon,
            });
          }
          window.center_map( map );

         }

        /*
        *  add_marker
        *  This function will add a marker to the selected Google Map
        */
        function add_marker( $marker, map ) {
          // var
          var latlngstring = $marker.attr('data-lat-lng');
          if (latlngstring == '') {
            return false;
          }
          var latlngarray = latlngstring.split(',');
          var lat = parseFloat(latlngarray[0]);
          var lng = parseFloat(latlngarray[1]);
          var latlng = new google.maps.LatLng( lat, lng );
          var pincolor = '#DD7F2A';
          var pinicon = pinSymbol(pincolor);
          /*if( $marker.attr('data-acct-type') === 'reseller-account-type-vantage-reseller'){
            pinicon = '/wp-content/plugins/ei-reseller-search/assets/images/vantage-map-marker.png';
          }*/
          //if facet is active make pins numbered
          var qs = FWP.build_query_string();
          if ( FWP.facets['reseller_radius_miles'].length > 0 || FWP.facets['reseller_radius_km'].length > 0 ) { 
            if (window.counter < 100) {
              pinicon = '/wp-content/plugins/ei-reseller-search/assets/images/pin-icons/marker_orange'+ window.counter +'.png';
            } else {
              pinicon = '/wp-content/plugins/ei-reseller-search/assets/images/pin-icons/marker_orange.png';
            }
            window.counter++;
          }
          // create marker
          var marker = new google.maps.Marker({
            position  : latlng,
            animation : google.maps.Animation.DROP,
            map       : map,
            icon: pinicon,
          });

          // add to array
          map.markers.push( marker );
          // Push the marker to the 'myMarkers' array that gets recreated after facet load
          window.myMarkers.push(marker);
          marker.addListener('click', function() {
            //map.setZoom(4);
            map.setCenter(marker.getPosition());
          });

          // if marker contains HTML, add it to an infoWindow
          if( $marker.html() )
          {
            // create info window
            var infowindow = new google.maps.InfoWindow({
